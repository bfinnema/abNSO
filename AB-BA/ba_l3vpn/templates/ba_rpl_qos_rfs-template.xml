<config-template xmlns="http://tail-f.com/ns/config/1.0">
  <ba_rpl_qos xmlns="http://example.com/ba_rpl_qos">
    <name>{$name}</name>
    <device>{$device}</device>
    <run_as_rfs>{$run_as_rfs}</run_as_rfs>
    <rfs>
      <interface>
        <policy_map_name>{$policy_map_name}</policy_map_name>
        <interface>
          <interface_name>{$interface_name}</interface_name>
          <interface_number>{$interface_number}</interface_number>
        </interface>
      </interface>
    </rfs>
  </ba_rpl_qos>
</config-template>
